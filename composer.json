{
    "name": "laravolt/email",
    "description": "Simple email management for User object",
    "keywords": [
        "laravolt",
        "email"
    ],
    "homepage": "https://github.com/laravolt/email",
    "license": "MIT",
    "authors": [
        {
            "name": "Bayu Hendra Winata",
            "email": "uyab.exe@gmail.com",
            "homepage": "http://id-laravel.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=5.5.0",
        "illuminate/support": "~5.1",
        "illuminate/eloquent": "~5.1"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*"
    },
    "autoload": {
        "psr-4": {
            "Laravolt\\Email\\": "src"
        }
    },
    "autoload-dev": {
